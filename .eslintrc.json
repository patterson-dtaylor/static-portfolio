{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
    "linebreak-style": [
        "error",
        "unix"
        ],
    "semi": [
        "error",
        "always"
    ],
    "no-console": [
        0
    ]
    }

}